</td>
  <h2>vai dar bom</h2>
<%= user_signed_in? %>
<%= current_user&.id %>
<% unless user_signed_in? %>
  <ul>
    <label>
      <%= button_to "login", first_login_path, method: :get, class:"btn btn-primary" %>
    </label>
    <label>
      <%= button_to "Cadastrar-se", new_user_registration_path, method: :get, class:"btn link" %>
