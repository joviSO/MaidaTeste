<p id="notice"><%= notice %></p>

<h1>Variation Weights</h1>

<table>
  <thead>
    <tr>
      <th>Weight</th>
      <th>Date</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @variation_weights.each do |variation_weight| %>
      <tr>
        <td><%= variation_weight.weight %></td>
        <td><%= variation_weight.date %></td>
        <td><%= link_to 'Show', variation_weight %></td>
        <td><%= link_to 'Edit', edit_variation_weight_path(variation_weight) %></td>
        <td><%= link_to 'Destroy', variation_weight, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Variation Weight', new_variation_weight_path %>
